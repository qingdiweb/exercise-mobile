<html ng-app="huazilive">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
    <meta name="wap-font-scale" content="no">
    <title>解析</title>
    <link href="css/ionic.css" rel="stylesheet">
    <link href="css/reset.css" rel="stylesheet">
    <link href="css/topic-cont.css" rel="stylesheet">
    <script type="text/javascript" src="js/ionic.bundle.min.js"></script>
    <script type="text/javascript" src="js/zepto.js"></script>
    <script type="text/javascript" src="js/app.js"></script>
    <!-- 题目内容 -->
    <script type="text/javascript" src="js/topic-cont/controller-parse.js"></script>
    <script type="text/javascript" src="js/topic-cont/directive-parse.js"></script>
    <script type="text/javascript" src="js/topic-cont/service.js"></script>
</head>
<body ng-controller="topicContParse" topic-cont-parse-directive>
    <div class="topicCont" overflow-scroll="true">
        <div class="pa loading-icon" ng-if="isLoading">
            <span class="loading-text">加载中</span>
            <ion-spinner icon="ios" style='display: block;text-align:center;'></ion-spinner>
        </div>
        <h1 class="head-nav" ng-if="navMask">
            <span class="title f14 fl wordbreak" ng-bind="chapterName"></span>
            <p class="current-topic f15 fr"><span ng-bind="parseCurrentTopic"></span>/<span ng-bind="questionCount"></span></p>
        </h1>
        <ion-slide-box class="view-slide-box" on-slide-changed="slideHasChanged($index,$event)" on-release="touchend()" on-swipe-left="swipeLeft()" delegate-handle="start">
          <ion-slide class="directory-slide first-directory-slide" ng-repeat="exerciseInfoList in exerciseQuestionInfoList" data-durationSecond="{{durationSecond}}" data-questionId="{{exerciseInfoList.questionId}}" data-isStart="{{exerciseInfoList.questionInfo.isCollected}}">
            <!-- 单选题 -->
            <div class="pa directory-topic-slide-box" ng-if="exerciseInfoList.questionInfo.type==0">
                <ion-scroll direction="y"  style="width: 100%;height: 100%" on-scroll="$event.preventDefault()">
                    <div class="directory-multi-topic-cont directory-radio-topic-cont directory-topic-public">
                        <p class="title-details f15"><span class="topic-type dib f14">(单选题)</span><span class="topic-type-txt" ng-bind-html="exerciseInfoList.questionInfo.title"></span></p>
                        <div class="topic-options"  data-exerciseId="{{exerciseInfoList.id}}" data-answer="{{exerciseInfoList.questionInfo.answer}}">
                            <div class="clearfix pr topic-options-box"  ng-repeat="optionsel in exerciseInfoList.questionInfo.option">
                                <b class="pa topic-options-mask"></b>
                                <span class="fl dib pr topic-options-letter">
                                    <a href="javascript:;" class="dib" ng-bind="optionsel.title"></a>
                                    <i class="pa" ng-class="exerciseInfoList.questionInfo.ParseAnswer==optionsel.title ? 'parse-right' : ''" ng-if="exerciseInfoList.questionInfo.ParseAnswer==optionsel.title"></i>
                                    <i class="pa" ng-class="exerciseInfoList.questionInfo.studentExerciseAnswerInfo.userAnswer==optionsel.title&&exerciseInfoList.questionInfo.studentExerciseAnswerInfo.userAnswer!=exerciseInfoList.questionInfo.ParseAnswer? 'parse-error' : ''" ng-if="exerciseInfoList.questionInfo.studentExerciseAnswerInfo.userAnswer==optionsel.title&&exerciseInfoList.questionInfo.studentExerciseAnswerInfo.userAnswer!=exerciseInfoList.questionInfo.ParseAnswer"></i>
                                </span>
                                <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                    
                                </p>
                            </div>
                        </div>
                        <!-- 解析 -->
                        <div class="topic-parse w fl">
                            <h2 class="topic-test-parsing-title w fl f14">
                                <span class="db tc">试题解析</span>
                            </h2>
                            <div class="right-answer w fl">
                                <span class="fl f15">正确答案</span><span class="right-answer-option  f16" ng-bind="exerciseInfoList.questionInfo.answerDetail|uppercase"></span>
                            </div>
                            <div class="fighting-pic fl f14 w" ng-if="exerciseInfoList.questionInfo.studentExerciseAnswerInfo.answer!=exerciseInfoList.questionInfo.answer">
                                <div class="fighting-pic-box"><span class="fl"></span>这个知识点还需要强化呀，加油!</div>
                            </div>
                            <div class="parse-cont-box fl w" ng-if="exerciseInfoList.questionInfo.answerParsing!=''&&exerciseInfoList.questionInfo.answerParsing!=null">
                                <p class="parse-cont-title w f15">解析</p>
                                <div class="parse-cont w f15" ng-bind-html="exerciseInfoList.questionInfo.answerParsing">
                                    
                                </div>
                            </div>
                            <div class="examination-site-box fl w" ng-if="exerciseInfoList.questionInfo.parseKnowledges.length!=0&&exerciseInfoList.questionInfo.parseKnowledges!=null">
                                <span class="examination-site-title fl f15">考点</span>
                                <p class="examination-site fr">
                                    <span class="fl f12" ng-repeat="parseKnow in exerciseInfoList.questionInfo.parseKnowledges">{{parseKnow}}</span>
                                </p>
                            </div>
                            <div class="title-feedback fr f12" on-tap="feedbackPopup($event)" data-questionId="{{exerciseInfoList.questionId}}">
                                题目反馈
                            </div>
                            <div style="float:left;width: 100%;height: 1rem;"></div>
                        </div>
                    </div>
                </ion-scroll>
            </div>
            <!-- 多选题 -->
            <div class="directory-topic-slide-box" ng-if="exerciseInfoList.questionInfo.type=='1'">
                <ion-scroll direction="y"  style="width: 100%;height: 100%" on-scroll="$event.preventDefault()">
                    <div class="directory-multi-topic-cont directory-checkout-topic-cont directory-topic-public">
                         <p class="title-details f15"><span class="topic-type dib f14">(多选题)</span><span class="topic-type-txt" ng-bind-html="exerciseInfoList.questionInfo.title"></span></p>
                        <div class="topic-options">
                            <div class="clearfix pr topic-options-box"  ng-repeat="optionsel in exerciseInfoList.questionInfo.option">
                                <b class="pa topic-options-mask"></b>
                                <span class="fl dib pr topic-options-letter">
                                    <a href="javascript:;" class="dib" ng-bind="optionsel.title"></a>
                                    <i class="pa" ng-class="exerciseInfoList.questionInfo.answer.search(optionsel.title)!=-1 ? 'parse-right' : ''" ng-if="exerciseInfoList.questionInfo.answer.search(optionsel.title)!=-1"></i>
                                    <i class="pa" ng-class="exerciseInfoList.questionInfo.studentExerciseAnswerInfo.differentOption.search(optionsel.title)!=-1? 'parse-error' : ''" ng-if="exerciseInfoList.questionInfo.studentExerciseAnswerInfo.differentOption!=''"></i>
                                </span>
                                <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                    
                                </p>
                            </div>
                        </div>
                        <!-- 解析 -->
                        <div class="topic-parse w fl">
                            <h2 class="topic-test-parsing-title w fl f14">
                                <span class="db tc">试题解析</span>
                            </h2>
                            <div class="right-answer w fl">
                                <span class="fl f15">正确答案</span><span class="right-answer-option f16" ng-repeat="ParseAnswer in exerciseInfoList.questionInfo.answerDetail|uppercase" ng-bind="ParseAnswer"></span>
                            </div>
                            <div class="fighting-pic fl f14 w" ng-if="exerciseInfoList.questionInfo.studentExerciseAnswerInfo.answer!=exerciseInfoList.questionInfo.answer">
                                <div class="fighting-pic-box"><span class="fl"></span>这个知识点还需要强化呀，加油!</div>
                            </div>
                            <div class="parse-cont-box fl w" ng-if="exerciseInfoList.questionInfo.answerParsing!=''&&exerciseInfoList.questionInfo.answerParsing!=null">
                                <p class="parse-cont-title w f15">解析</p>
                                <div class="parse-cont w f15" ng-bind-html="exerciseInfoList.questionInfo.answerParsing">
                                    
                                </div>
                            </div>
                            <div class="examination-site-box fl w" ng-if="exerciseInfoList.questionInfo.parseKnowledges.length!=0&&exerciseInfoList.questionInfo.parseKnowledges!=null">
                                <span class="examination-site-title fl f15">考点</span>
                                <p class="examination-site fr">
                                    <span class="fl f12" ng-repeat="parseKnow in exerciseInfoList.questionInfo.parseKnowledges">{{parseKnow}}</span>
                                </p>
                            </div>
                            <div class="title-feedback fr f12" on-tap="feedbackPopup($event)" data-questionId="{{exerciseInfoList.questionId}}">
                                题目反馈
                            </div>
                            <div style="float:left;width: 100%;height: 1rem;"></div>
                        </div>
                    </div>
                </ion-scroll>
            </div>
            <!-- 填空题和问答题 -->
            <div class="directory-topic-slide-box" ng-if="exerciseInfoList.questionInfo.type==2||exerciseInfoList.questionInfo.type==3">
