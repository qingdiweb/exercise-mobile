	                                        <!-- 单选题 -->
	                                        <div class="pa directory-topic-slide-box" ng-if="questionInfoList.type==0">
	                                            <ion-scroll zooming="false" direction="y"  style="width: 100%;height: 100%" on-scroll="$event.preventDefault()">
