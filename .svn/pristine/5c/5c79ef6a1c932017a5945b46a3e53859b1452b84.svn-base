<html ng-app="huazilive">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
    <meta name="wap-font-scale" content="no">
    <title>错题本</title>
    <link href="css/ionic.css" rel="stylesheet">
    <link href="css/reset.css" rel="stylesheet">
    <link href="css/edit.css" rel="stylesheet">
    <link href="css/topic-cont.css" rel="stylesheet">
  <!--   <script src="js/angular/angular.min.js"></script>
  <script src="js/angular-ui/angular-ui-router.min.js"></script> -->
    <script type="text/javascript" src="js/ionic.bundle.min.js"></script>
    <script type="text/javascript" src="js/zepto.js"></script>
    <script type="text/javascript" src="js/app.js"></script>
    <!-- 题目内容 -->
    <script type="text/javascript" src="js/topic-cont/controller-wrong.js"></script>
    <script type="text/javascript" src="js/topic-cont/directive-wrong.js"></script>
    <script type="text/javascript" src="js/topic-cont/service.js"></script>
</head>
    <body style="background:rgba(240,240,242,1);" ng-controller="topicWrongCont" topic-wrong-cont-directive>
        <ion-content class="wrong-cont-box w fl" overflow-scroll="true">
          <!--   <span>{{questionId}}</span> -->
            <div class="pa loading-icon" ng-if="isLoading">
                <span class="loading-text">加载中</span>
                <ion-spinner icon="ios" style='display: block;text-align:center;'></ion-spinner>
            </div>
            <ion-scroll zooming="false" direction="y"  style="width: 100%;height: 100%;" on-scroll="$event.preventDefault()">
                <div class="w fl">
            	    <div  class="directory-slide directory-slide-list fl w" ng-repeat="wrongContList in wrongCont" data-index="{{$index}}" data-questionId="{{wrongContList.questionId}}" data-isStart="" on-tap="jumpParse($event)">
                        <!-- 单选题 -->
                        <div class="directory-topic-slide-box wrong-topic-slide-box mb10 bgf" ng-if="wrongContList.questionInfo.type==0">
                            <div class="directory-multi-topic-cont directory-radio-topic-cont directory-topic-public mt0">
                                <p class="title-details f15"><span class="topic-type dib f14">(单选题)</span><span class="topic-type-txt" ng-bind-html="wrongContList.questionInfo.title"></span></p>
                                <div class="topic-options"  data-paperId="{{wrongContList.id}}" data-answer="{{wrongContList.questionInfo.answer}}">
                                    <div class="clearfix pr topic-options-box"  ng-repeat="optionsel in wrongContList.questionInfo.option">
                                        <b class="pa topic-options-mask"></b>
                                        <span class="fl dib pr topic-options-letter" ng-bind="optionsel.title"></span>
                                        <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                            
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- 多选题 -->
                        <div class="directory-topic-slide-box wrong-topic-slide-box mt0 mb10 bgf" ng-if="wrongContList.questionInfo.type==1">
                            <div class="directory-multi-topic-cont directory-checkout-topic-cont directory-topic-public mt0">
                                 <p class="title-details f15"><span class="topic-type dib f14">(多选题)</span><span class="topic-type-txt" ng-bind-html="wrongContList.questionInfo.title"></span></p>
                                <div class="topic-options" data-paperId="{{wrongContList.id}}" data-answer="{{wrongContList.questionInfo.answer}}">
                                    <div class="clearfix pr topic-options-box"  ng-repeat="optionsel in wrongContList.questionInfo.option">
                                        <b class="pa topic-options-mask"></b>
                                        <span class="fl dib pr topic-options-letter" ng-bind="optionsel.title"></span>
                                        <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                            
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- 填空题 -->
                        <div class="directory-topic-slide-box wrong-topic-slide-box mt0 mb10 bgf"  ng-if="wrongContList.questionInfo.type==2">
                            <div class="directory-fillUp-topic-cont pr directory-topic-public mt0">
                                <p class="title-details f15"><span class="topic-type dib f14">(填空题)</span><span class="topic-type-txt" ng-bind-html="wrongContList.questionInfo.title"></span></p>
                                 <div class="topic-fillUp f14">
                                    本题目不支持作答,可在交卷后核对答案并查看解析
                                </div>
                            </div>
                        </div>
                        <!-- 问答题 -->
                        <div class="directory-topic-slide-box wrong-topic-slide-box mt0 mb10 bgf" ng-if="wrongContList.questionInfo.type==3">
                            <div class="directory-fillUp-topic-cont pr directory-topic-public mt0">
                                <p class="title-details f15"><span class="topic-type dib f14">(问答题)</span><span class="topic-type-txt" ng-bind-html="wrongContList.questionInfo.title"></span></p>
                                
                                 <div class="topic-fillUp f14">
                                    本题目不支持作答,可在交卷后核对答案并查看解析
                                </div>
                            </div>
                        </div>
                        <!-- 多问题 -->
                        <div class="directory-topic-slide-box wrong-topic-slide-box mt0 mb10 bgf" ng-if="wrongContList.questionInfo.type==4">
                            <div class="directory-readComprehen-topic-cont pr directory-topic-public mt0">
                                <div class="title-details" style="width: 100%;;">
                                        <div>
                                            <p ng-bind-html="wrongContList.questionInfo.title"></p>
                                            <div class="blank-box"></div> 
                                        </div>
                                </div>
                            </div>
                        </div>
                        <!-- 短听力 -->
