}
/*听力*/
.directory-hearing-topic-cont{
	margin:0.18rem 0.21rem 0 0.23rem;
}
.directory-hearing-topic-cont .hearing-auto{
	background: skyblue;
}
.directory-hearing-topic-cont .title-details,.directory-longhearing-topic-cont .title-details{
	margin-top:0.41rem;
	color: #444444;
	line-height: 0.48rem;
}
.directory-hearing-topic-cont .title-details .topic-type,.directory-longhearing-topic-cont .title-details .topic-type{
	margin-right:0.17rem; 
	color: #FF8548;
}
/*答题卡*/
.sheet-box{
	width: 100%;
	height: 100%;
}
.sheet-select{
	margin:30px 37px 0 8px;
}
.sheet-select .topic{
	width: 25px;
	height: 25px;
	margin:0 0 20px 30px;
	text-align: center;
	line-height: 25px;
	border-radius: 50%;
}
.sheet-select .topic-no-haveTo-answer{
	color: #444;
	background: url(../img/option.png) no-repeat;
	background-size:100% 100%;
}
.topic-no-answer{
	color: #2DBB55;
	background: url(../img/option.png) no-repeat;
	background-size:100% 100%;
}
.topic-answer{
	color: #FFF;
	background: url(../img/selected.png) center center no-repeat;
	background-size:100% 100%;
}
.sheet-submit-paper{
	position: fixed;
	left: 5%;
	bottom:0.21rem;
	width: 90%;
	height: 0.60rem;
	color: #FFF;
	background: #2DBB55;
	border-radius: 2px;
	border: 0px;
	outline: none;
}

/*解析*/
.topic-parse{
	margin-top: 0.68rem;
}
.topic-parse .topic-test-parsing-title{
	height: 1px;
	color: #707477;
	background: url(../img/line.png) no-repeat;
}
.topic-parse .topic-test-parsing-title span{
	width: 1.20rem;
	margin: -0.14rem auto;
	background: #fff;
}
.topic-parse .right-answer{
	margin: 0.3rem 0 0.21rem 0;
}
.topic-parse .right-answer span{
	color: #444;
}
 .topic-parse .right-answer .right-answer-option{
 	float: left;
 	width: 100%;
	margin:0.1rem 0 0 0;
	font-size: 0.26rem;
	line-height: .5rem;
} 
.topic-parse .right-answer .choice-right-answer-option{
	float: left;
	margin:0 0 0 0.1rem;
	font-size: 0.26rem;
} 

.topic-parse .right-answer .right-answer-option p{
	display: inherit;
}
.topic-parse .right-answer .right-answer-option img{
	vertical-align:middle;
}
.topic-parse .fighting-pic{
	height: 16px;
	margin: 0 auto 0.24rem;
	color: #FF8548;
	line-height: 16px;
}
.topic-parse .fighting-pic span{
	width: 0.27rem;
	height: 0.27rem;
	margin-right: 0.14rem;
	background: url(../img/fail.png) left center no-repeat;
	background-size:100% 100%; 
}
.topic-parse .fighting-pic-box{
	width: 3.9rem;
	height: 16px;
	margin:0 auto;
	line-height: 16px;
}
.topic-parse .parse-cont-title{
	color: #444;
}
.topic-parse .parse-cont{
	margin:0.1rem 0 0.34rem 0;
	color: #444;
	line-height: 0.5rem;
}
.topic-parse .examination-site-title{
	width: 0.6rem;
	color: #444;
	margin-top: 0.03rem;
}
.topic-parse .examination-site{
	width: calc(100% - 0.7rem);
	margin-top: 0.02rem;
}
.topic-parse .examination-site span{
	margin: 0 0.2rem 0.17rem 0;
	padding: 2px 6px;
	color:#2DBB55;
	border: 1px solid #2DBB55;
	border-radius: 4px;  
}
.topic-parse .title-feedback{
	width: 1.09rem;
	height: 0.41rem;
	margin-top: 0.2rem;
	color: #707477;
	border: 1px solid #DBDBDB;
	border-radius: 4px;
	text-align: center;
	line-height: 0.40rem;
}
@media screen and (min-width: 480px){
	.topic-parse .title-feedback{
		width: 1.09rem;
		height: 0.41rem;
		margin-top: 0.2rem;
		color: #707477;
		border: 1px solid #DBDBDB;
		border-radius: 4px;
		text-align: center;
		line-height: 0.41rem;
	}
}
.topic-parse .title-delete{
	margin-top: 0.2rem;
	color: #707477;
	border-radius: 0.27rem;
	text-align: center;
	line-height: 0.37rem;
}
.topic-parse .title-delete span{
	width: 0.24rem;
	height: 0.24rem;
	margin:0.05rem 0.1rem 0 0;
	background: url(../img/del.png) center center no-repeat;
	background-size:100% 100%; 
}
.directory-slide-one{
	left:0 !important;
}
.directory-slide-two{
	left:-20% !important;
}
.directory-slide-three{
	left:-40% !important;
}
.directory-slide-four{
	left:-60% !important;
}
.directory-slide-five{
	left:-80% !important;
}
/*试卷*/
.paper-type-title{
	padding: 0.21rem 0.24rem 0 0.27rem;
}
/*错题本*/
.wrong-topic-slide-box{
	height: auto;
	padding: 0.3rem 0 0.4rem 0;
}
.wrong-topic-slide-box .directory-multi-topic-cont {
    height: auto;
    margin: 0.21rem 0.24rem 0 0.27rem;
    display: table;
	content: '';
	clear: both;
}
.loading-icon{
	left:50%;
	top:50%;
	-webkit-transform:translate(-50%,-50%); 
	display: block;
}
.loading-text{
	display: block;
	color:#707477;
}
.wrongCollectLoding{
	width: 100%;
	height: 100%;
	background: #fff;
}
