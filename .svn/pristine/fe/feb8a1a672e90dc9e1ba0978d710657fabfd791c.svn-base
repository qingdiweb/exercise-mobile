/*多问题*/
.directory-readComprehen-topic-cont{
	margin:0rem 0.24rem 0 0.27rem;
	padding-top:0.21rem; 
}
.directory-readComprehen-topic-cont .title-details .topic-type{
	margin-bottom: 0.1rem;
	margin-right:0.17rem; 
	color: #FF8548;
}
.directory-read-multiple-choice{
	z-index: 100;
	top:4.27rem;
	left: -0.24rem;
	width: calc(100% + 0.51rem);
	height: calc(100% - 4.27rem);
	/* height: 100%; */
	background: #fff;
}
.updownMoveBtn{
	z-index: 1000;
	left: calc(50% - 0.24rem);
	width: 1.4rem;
	height: 1.4rem;
	margin:-0.84rem 0 0 -0.43rem;
}
.updownMoveBtn i{
	left: 50%;
	top: 50%;
	width: 1.03rem;
	height: 0.34rem;
	margin:-0.19rem 0 0 -0.515rem;
	background: url(../img/slide-icon.png) no-repeat;
	background-size:100% 100%;
}
.blank-box{
	width: 100%;
	height: 1.71rem;
}
.doubleview-directory-read-multiple-choice .double-view-slide{
	/* height: 4.8rem; */
	/* height: calc(100% - 5.04rem); */
	height: 100%;
