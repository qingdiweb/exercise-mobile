/*公共css*/
.directory-topic-public .title-details .topic-type-txt{
	color: #444;
}
.directory-topic-public .title-details .topic-type-txt p{
	position: relative;
	display: inherit;
}
.directory-topic-public .title-details .topic-type-txt img{
	vertical-align:top;
}
.directory-topic-public .title-details .topic-type-txt div{
	display: inherit;
}
/*公共css*/
.directory-topic-slide-box{
	width: 100%;
	height: 100%;
}
.head-nav{
	width: 100%;
	height: 0.77rem;
	background: url(../img/line.png) center bottom no-repeat,url(../img/line.png) center top no-repeat;
}
.sheet-head-nav{
	width: 100%;
	height: 0.77rem;
}
.doubleView-head-nav{
	width: 100%;
	height: 0.77rem;
	background: url(../img/line.png) center bottom no-repeat,url(../img/line.png) center top no-repeat;
}
.head-nav .title{
	width: 4.98rem;
	/* height: 0.25rem; */
	margin:0.27rem 0 0 0.27rem;
	color: #707477;
	/* line-height: 0.26rem; */
}
.head-nav .doubleViewTitle{
	width: 4.6rem;
	height: 0.25rem;
	margin:0.27rem 0 0 0.27rem;
	color: #707477;
	line-height: 0.26rem;
}
.doubleView-head-nav .title{
	margin:0.27rem 0 0 0rem;
}
.sheet-head-nav .title{
	margin:0.27rem 0 0 0.27rem;
	color: #707477;
	line-height: 0.26rem;
}
.head-nav .current-topic{
	margin:0.26rem 0.28rem 0 0;
	line-height: 0.26rem;
	letter-spacing: 0.017rem;
}
.head-nav .current-topic span:nth-child(1){
	color:#444;
}
.head-nav .current-topic span:nth-child(2){
	color:#B1B5B8;
}
.directory-topic-slide-box .directory-multi-topic-cont,.pdf-question-list .directory-multi-topic-cont{
	height: auto;
	margin:0.21rem 0.24rem 0 0.27rem;
	display: table;
	content: '';
	clear: both;
}
.doubleview-directory-topic-slide-box .directory-multi-topic-cont{
	height: auto;
	display: table;
	content: '';
	clear: both;
}
.directory-multi-topic-cont .title-details{
	color: #444444;
	line-height: 0.48rem;
}
.directory-multi-topic-cont .title-details .topic-type{
	margin-right:0.17rem; 
	color: #FF8548;
}
.directory-multi-topic-cont .topic-options{
	width:100%;
	color: #444;
	margin-top: 0.3rem;
}
.directory-multi-topic-cont .topic-options-box{
	margin-top: 0.2rem;
}
.directory-multi-topic-cont .topic-options-mask{
	z-index: 100;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}
.directory-multi-topic-cont .topic-options .topic-options-letter{
	width: 0.55rem;
	height: 0.55rem;
	text-align: center;
	line-height: 0.55rem;
	font-size: 0.26rem;
	color: #444;
	background: url(../img/option.png) no-repeat;
	background-size:100% 100%;
	border-radius:50%;
}
.directory-multi-topic-cont .topic-options .topic-options-letter a{
	width: 100%;
	height: 100%;
	text-align: center;
	line-height: 0.55rem;
	font-size: 0.26rem;
	color: #444;
	border-radius:50%;
}
.directory-multi-topic-cont .topic-options .topic-options-letter i{
	z-index: 100;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	text-align: center;
	line-height: 0.55rem;
	font-size: 0.26rem;
	color: #444;
	border-radius:50%;
}
.directory-multi-topic-cont .topic-options .topic-options-letter.selected{
	color: #fff;
	background: url(../img/selected.png) no-repeat;
	background-size:100% 100%;
	border-radius:50%;
}
.directory-multi-topic-cont .topic-options .topic-options-letter i.parse-right{
	background: url(../img/correct.png) no-repeat;
	background-size:100% 100%;
}
.directory-multi-topic-cont .topic-options .topic-options-letter i.parse-error{
	background: url(../img/error.png) no-repeat;
	background-size:100% 100%;
}
.directory-multi-topic-cont .topic-options .topic-options-selected{
	z-index: 100;
	left: 0;
	top: 0;
	display: none;
	width: 0.43rem;
	height: 0.43rem;
	background: url(../img/selected.png) center center no-repeat;
	background-size: 100% 100%; 
	border-radius: 50%;
}
.directory-multi-topic-cont .topic-options .topic-options-details{
	margin: 0.1rem 0 0 0.68rem;
	color:#444;
	line-height: 0.35rem;
}
.slider-pager{
	display: none !important;
}
