<html ng-app="huazilive">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
    <meta name="wap-font-scale" content="no">
    <title>分享试题</title>
    <link href="css/ionic.css" rel="stylesheet">
    <link href="css/reset.css" rel="stylesheet">
    <link href="css/edit.css" rel="stylesheet">
    <link href="css/topic-cont.css" rel="stylesheet">
    <script type="text/javascript" src="js/ionic.bundle.min.js"></script>
    <script type="text/javascript" src="js/zepto.js"></script>
    <script type="text/javascript" src="js/app.js"></script>
    <!-- 题目内容 -->
    <script type="text/javascript" src="js/topic-cont/controller-share-parse.js"></script>
    <script type="text/javascript" src="js/topic-cont/directive-share-parse.js"></script>
    <script type="text/javascript" src="js/topic-cont/service.js"></script>
</head>

<body ng-controller="topicShareParse" topic-share-parse-directive>
    <div class="topicCont" overflow-scroll="true" ng-cloak>
        <div class="directory-slide first-directory-slide share-parse-box">
            <!-- 单选题 -->
            <div class="pa directory-topic-slide-box" ng-if="exerciseInfoList.type==0">
                    <div class="directory-multi-topic-cont directory-radio-topic-cont directory-topic-public">
                        <div class="hidden-box">
                            <p class="title-details f15"><span class="topic-type dib f14" ng-bind="'(单选题)'"></span><span class="topic-type-txt" ng-bind-html="exerciseInfoList.title"></span></p>
                            <div class="topic-options"  data-exerciseId="{{exerciseInfoList.id}}" data-answer="{{exerciseInfoList.answer}}">
                                <div class="clearfix pr topic-options-box"  ng-repeat="optionsel in exerciseInfoList.option">
                                    <b class="pa topic-options-mask"></b>
                                    <span class="fl dib pr topic-options-letter">
                                        <a href="javascript:;" class="dib" ng-bind="optionsel.title"></a>
                                        <i class="pa"></i>
                                    </span>
                                    <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                        
                                    </p>
                                </div>
                            </div>
                        </div>
                        
                        <!-- 解析 -->
                        <div class="topic-parse w fl" ng-cloak>
                            <h2 class="topic-test-parsing-title w fl f14" ng-cloak>
                                <span class="db tc" ng-bind="'试题解析'"></span>
                            </h2>
                            <div class="share-topic-parse w fl">
                                <div class="right-answer w fl">
                                    <span class="fl f15" ng-bind="'正确答案'"></span><span ng-class="wrongContList.answerDetail.length>5 ? 'right-answer-option f16' : 'choice-right-answer-option f16'" ng-bind-html="exerciseInfoList.answerDetail"></span>
                                </div>
                               
                                <div class="parse-cont-box fl w" ng-if="exerciseInfoList.answerParsing!=''&&exerciseInfoList.answerParsing!=null">
                                    <p class="parse-cont-title w f15" ng-bind="'解析'"></p>
                                    <div class="parse-cont w f15" ng-bind-html="exerciseInfoList.answerParsing">
                                        
                                    </div>
                                </div>
                                <div class="examination-site-box fl w" ng-if="exerciseInfoList.parseKnowledges.length!=0&&exerciseInfoList.parseKnowledges!=null">
                                    <span class="examination-site-title fl f15" ng-bind="'考点'"></span>
                                    <p class="examination-site fr" ng-cloak>
                                        <span class="fl f12" ng-repeat="parseKnow in exerciseInfoList.parseKnowledges" ng-bind="parseKnow"></span>
                                    </p>
                                </div>
                            </div>
                            
                        </div>
                    </div>
            </div>
            <!-- 多选题 -->
            <div class="directory-topic-slide-box" ng-if="exerciseInfoList.type=='1'">
                    <div class="directory-multi-topic-cont directory-checkout-topic-cont directory-topic-public">
                         <div class="hidden-box">
                                  <p class="title-details f15"><span class="topic-type dib f14" ng-bind="'(多选题)'"></span><span class="topic-type-txt" ng-bind-html="exerciseInfoList.title"></span></p>
                            <div class="topic-options">
                                <div class="clearfix pr topic-options-box"  ng-repeat="optionsel in exerciseInfoList.option">
                                    <b class="pa topic-options-mask"></b>
                                    <span class="fl dib pr topic-options-letter">
                                        <a href="javascript:;" class="dib" ng-bind="optionsel.title"></a>
                                        <i class="pa"></i>
                                    </span>
                                    <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                        
                                    </p>
                                </div>
                            </div>
                         </div>
                        
                        <!-- 解析 -->
                        <div class="topic-parse w fl" ng-cloak>
                            <h2 class="topic-test-parsing-title w fl f14" ng-cloak>
                                <span class="db tc" ng-bind="'试题解析'"></span>
                            </h2>
                            <div class="share-topic-parse w fl">
                                <div class="right-answer w fl">
                                    <span class="fl f15" ng-bind="'正确答案'"></span><span ng-class="wrongContList.answerDetail.length>5 ? 'right-answer-option f16' : 'choice-right-answer-option f16'" ng-bind-html="exerciseInfoList.answerDetail"></span>
                                </div>
                               
                                <div class="parse-cont-box fl w" ng-if="exerciseInfoList.answerParsing!=''&&exerciseInfoList.answerParsing!=null">
                                    <p class="parse-cont-title w f15" ng-bind="'解析'"></p>
                                    <div class="parse-cont w f15" ng-bind-html="exerciseInfoList.answerParsing">
                                        
                                    </div>
                                </div>
                                <div class="examination-site-box fl w" ng-if="exerciseInfoList.parseKnowledges.length!=0&&exerciseInfoList.parseKnowledges!=null">
                                    <span class="examination-site-title fl f15" ng-bind="'考点'"></span>
                                    <p class="examination-site fr" ng-cloak>
                                        <span class="fl f12" ng-repeat="parseKnow in exerciseInfoList.parseKnowledges" ng-bind="parseKnow"></span>
                                    </p>
                                </div>
                            </div>

                            
                        </div>
                    </div>
            </div>
            <!-- 填空题和问答题 -->
            <div class="directory-topic-slide-box" ng-if="exerciseInfoList.type==2||exerciseInfoList.type==3">
                    <div class="directory-fillUp-topic-cont directory-topic-public pr">
                        <div class="hidden-box">
                            <p class="title-details f15"><span class="topic-type dib f14" ng-if="exerciseInfoList.type==2" ng-bind="'(填空题)'"></span><span class="topic-type dib f14" ng-if="exerciseInfoList.type==3" ng-bind="'(简答题)'"></span><span class="topic-type-txt" ng-bind-html="exerciseInfoList.title"></span></p>
                        </div>
                        
                        <div class="topic-parse w fl" ng-cloak>
                            <h2 class="topic-test-parsing-title w fl f14" ng-cloak>
                                <span class="db tc" ng-bind="'试题解析'"></span>
                            </h2>
                            <div class="share-topic-parse w fl">
                                <div class="right-answer w fl">
                                    <span class="fl f15" ng-bind="'正确答案'"></span><span class="right-answer-option  f16" ng-bind-html="exerciseInfoList.answerDetail"></span>
                                </div>
                                <div class="parse-cont-box fl w" ng-if="exerciseInfoList.answerParsing!=''&&exerciseInfoList.answerParsing!=null">
                                    <p class="parse-cont-title w f15" ng-bind="'解析'"></p>
                                    <div class="parse-cont w f15" ng-bind-html="exerciseInfoList.answerParsing">
                                        
                                    </div>
                                </div>
                                <div class="examination-site-box fl w" ng-if="exerciseInfoList.parseKnowledges.length!=0&&exerciseInfoList.parseKnowledges!=null">
                                    <span class="examination-site-title fl f15" ng-bind="'考点'"></span>
                                    <p class="examination-site fr" ng-cloak>
                                        <span class="fl f12" ng-repeat="parseKnow in exerciseInfoList.parseKnowledges" ng-bind="parseKnow"></span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>
            <!-- 多问题 -->
            <div class="directory-topic-slide-box doubleview-directory-topic-slide-box graybgf" ng-if="exerciseInfoList.type==4">
                    <div class="directory-readComprehen-topic-cont pr directory-topic-public">
                        <div class="hidden-box">
                            <div class="title-details" style="width: 100%;height: 100%;">
                                <div>
                                    <p ng-bind-html="exerciseInfoList.title"></p>
                                </div>
                             </div>
                        </div>
                        <div class="pa directory-read-multiple-choice doubleview-directory-read-multiple-choice">
                            <span class="db pa updownMoveBtn">
                                    <i class="db pa"></i>
                            </span>
                            <div overflow-scroll="true">
                                <h1 class="head-nav">
                                    <span class="title doubleViewTitle f14 fl wordbreak" ng-bind="'多问题'"></span>
                                    <p class="current-topic doubleViewCurrentTopic f15 fr"><span class="first-view-page double-view-page" ng-bind="doubleViewTopic"></span>/<span class="double-view-num" data-num="{{exerciseInfoList.doubleViewTopicNum}}" ng-bind="exerciseInfoList.doubleViewTopicNum"></span></p>
                                </h1>
                                <ion-slide-box class="double-view-slide" on-slide-changed="slideReadChanged($index,$event)" delegate-handle="viewstart">
                                    <ion-slide class="directory-slide" ng-repeat="questionInfoList in exerciseInfoList.childQuestionInfoList">
                                    <div class="directory-slide">
                                        <!-- 单选题 -->
                                        <div class="pa directory-topic-slide-box" ng-if="questionInfoList.type==0">
                                                <div class="directory-multi-topic-cont directory-radio-topic-cont directory-topic-public clearfix">
                                                    <p class="title-details f15"><span class="topic-type dib f14" ng-bind="'(单选题)'"></span><span class="topic-type-txt" ng-bind-html="questionInfoList.title"></span></p>
                                                    <div class="topic-options"  data-exerciseId="{{questionInfoList.exerciseId}}" data-answer="{{questionInfoList.answer}}">
                                                        <div class="clearfix pr topic-options-box" on-touch="onRadioTouch($event)" ng-repeat="optionsel in questionInfoList.option">
                                                            <b class="pa topic-options-mask"></b>
                                                             <span class="fl dib pr topic-options-letter">
                                                                <a href="javascript:;" class="dib" ng-bind="optionsel.title"></a>
                                                                <i class="pa"></i>                                                         
                                                            </span>
                                                            <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                                                
                                                            </p>
                                                        </div>
                                                    </div>
                                                    <!-- 解析 -->
                                                    <div class="topic-parse w fl" ng-cloak>
                                                        <h2 class="topic-test-parsing-title w fl f14" ng-cloak>
                                                            <span class="db tc" ng-bind="'试题解析'"></span>
                                                        </h2>
                                                        <div class="right-answer w fl">
                                                            <span class="fl f15" ng-bind="'正确答案'"></span><span ng-class="wrongContList.answerDetail.length>5 ? 'right-answer-option f16' : 'choice-right-answer-option f16'" ng-bind-html="questionInfoList.answerDetail"></span>
                                                        </div>
                                                        <div class="parse-cont-box fl w" ng-if="questionInfoList.answerParsing!=''&&questionInfoList.answerParsing!=null">
                                                            <p class="parse-cont-title w f15" ng-bind="'解析'"></p>
                                                            <div class="parse-cont w f15" ng-bind-html="questionInfoList.answerParsing">
                                                                
                                                            </div>
                                                        </div>
                                                        <div class="examination-site-box fl w" ng-if="questionInfoList.parseKnowledges.length!=0&&questionInfoList.parseKnowledges!=null">
                                                            <span class="examination-site-title fl f15" ng-bind="'考点'"></span>
                                                            <p class="examination-site fr" ng-cloak>
                                                                <span class="fl f12" ng-repeat="parseKnow in questionInfoList.parseKnowledges" ng-bind="parseKnow"></span>
                                                            </p>
                                                        </div>
                                                        <div class="title-feedback fr f12" on-tap="feedbackPopup($event)" data-questionId="{{exerciseInfoList.questionId}}" ng-bind="'题目反馈'">
                                                            
                                                        </div>
                                                        <div style="float:left;width: 100%;height: 1rem;"></div>
                                                    </div>
                                                </div>
                                        </div>
                                        <!-- 多选题 -->
                                        <div class="directory-topic-slide-box" ng-if="questionInfoList.type==1">
                                                <div class="directory-multi-topic-cont directory-checkout-topic-cont directory-topic-public clearfix">
                                                     <p class="title-details f15"><span class="topic-type dib f14" ng-bind="'(多选题)'"></span><span class="topic-type-txt" ng-bind-html="questionInfoList.title"></span></p>
                                                    <div class="topic-options"  data-exerciseId="{{questionInfoList.exerciseId}}" data-answer="{{questionInfoList.answer}}">
                                                        <div class="clearfix pr topic-options-box" on-tap="onCheckTouch($event)" ng-repeat="optionsel in questionInfoList.option">
                                                            <b class="pa topic-options-mask"></b>
                                                            <span class="fl dib pr topic-options-letter">
                                                                <a href="javascript:;" class="dib" ng-bind="optionsel.title"></a>
                                                                <i class="pa"></i>
                                                            </span>
                                                            <p class="f15 topic-options-details" ng-bind-html="optionsel.option">
                                                                
                                                            </p>
                                                        </div>
                                                    </div>
                                                    <!-- 解析 -->
                                                    <div class="topic-parse w fl" ng-cloak>
                                                        <h2 class="topic-test-parsing-title w fl f14" ng-cloak>
                                                            <span class="db tc" ng-bind="'试题解析'"></span>
                                                        </h2>
                                                        <div class="right-answer w fl">
                                                            <span class="fl f15" ng-bind="'正确答案'"></span><span ng-class="wrongContList.answerDetail.length>5 ? 'right-answer-option f16' : 'choice-right-answer-option f16'" ng-bind-html="questionInfoList.answerDetail"></span>
                                                        </div>
                                                        <div class="parse-cont-box fl w" ng-if="questionInfoList.answerParsing!=''&&questionInfoList.answerParsing!=null">
                                                            <p class="parse-cont-title w f15" ng-bind="'解析'"></p>
                                                            <div class="parse-cont w f15" ng-bind-html="questionInfoList.answerParsing">
                                                                
                                                            </div>
                                                        </div>
                                                        <div class="examination-site-box fl w" ng-if="questionInfoList.parseKnowledges.length!=0&&questionInfoList.parseKnowledges!=null">
                                                            <span class="examination-site-title fl f15" ng-bind="'考点'"></span>
                                                            <p class="examination-site fr" ng-cloak>
                                                                <span class="fl f12" ng-repeat="parseKnow in questionInfoList.parseKnowledges" ng-bind="parseKnow"></span>
                                                            </p>
                                                        </div>
                                                        <div class="title-feedback fr f12" on-tap="feedbackPopup($event)" data-questionId="{{exerciseInfoList.questionId}}" ng-bind="'题目反馈'">
                                                            
                                                        </div>
                                                        <div style="float:left;width: 100%;height: 1rem;"></div>
                                                    </div>
                                                </div>
                                        </div>
                                        <!-- 填空题和问答题 -->
                                        <div class="directory-topic-slide-box" ng-if="questionInfoList.type==2||questionInfoList.type==3">
                                                <div class="directory-fillUp-topic-cont pr directory-topic-public clearfix">
                                                    <p class="title-details f15"><span class="topic-type dib f14" ng-if="questionInfoList.type==2" ng-bind="'(填空题)'"></span><span class="topic-type dib f14" ng-if="questionInfoList.type==3" ng-bind="'(简答题)'"></span><span class="topic-type-txt" ng-bind-html="questionInfoList.title"></span></p>
                                                   
                                                    <!-- 解析 -->
                                                    <div class="topic-parse w fl" ng-cloak>
                                                        <h2 class="topic-test-parsing-title w fl f14" ng-cloak>
                                                            <span class="db tc" ng-bind="'试题解析'"></span>
                                                        </h2>
                                                        <div class="right-answer w fl">
                                                            <span class="fl f15" ng-bind="'正确答案'"></span><span class="right-answer-option f16" ng-bind-html="questionInfoList.answerDetail"></span>
                                                        </div>
                                                        <div class="parse-cont-box fl w" ng-if="questionInfoList.answerParsing!=''&&questionInfoList.answerParsing!=null">
                                                            <p class="parse-cont-title w f15" ng-bind="'解析'"></p>
                                                            <div class="parse-cont w f15" ng-bind-html="questionInfoList.answerParsing">
                                                                
                                                            </div>
                                                        </div>
                                                        <div class="examination-site-box fl w" ng-if="questionInfoList.parseKnowledges.length!=0&&questionInfoList.parseKnowledges!=null">
                                                            <span class="examination-site-title fl f15" ng-bind="'考点'"></span>
                                                            <p class="examination-site fr" ng-cloak>
                                                                <span class="fl f12" ng-repeat="parseKnow in questionInfoList.parseKnowledges" ng-bind="parseKnow"></span>
                                                            </p>
                                                        </div>
                                                        <div class="title-feedback fr f12" on-tap="feedbackPopup($event)" data-questionId="{{exerciseInfoList.questionId}}" ng-bind="'题目反馈'">
                                                            
                                                        </div>
                                                        <div style="float:left;width: 100%;height: 1rem;"></div>
                                                    </div>
                                                </div>
                                        </div>
                                    </div>
                                    </ion-slide>
                                </ion-slide-box>
                            </div>
                        </div>
                    </div>
            </div>
        </div> 
        <div class="open-oula-mask">
            <a href="javascript:;" class="open-oula-btn f16" on-touch="openAPP()">打开欧拉教育,&nbsp;&nbsp;查看完整解析</a>
        </div>
        <div class="open-oula-app">
                <span class="open-app-text f14">欧拉教育</span>
                <a href="javascript:;" class="immediately-open f16" on-touch="openAPP()">APP内打开</a>
        </div>
    </div>
    </body>
</html>
